{
    "name": "daniel-km/noid",
    "description": "Create and manage temporary or permanent nice opaque identifiers for physical or digital objects like `12025/654xz321` (port of a perl utility).",
    "type": "library",
    "license": "CECILL-B",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        },
        {
            "name": "John A. Kunze",
            "email": "jak@ucop.edu"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "minimum-stability": "stable",
    "keywords": [
        "Noid4Php",
        "identifier",
        "utility",
        "nice opaque identifier",
        "php",
        "perl",
        "phpunit",
        "noid",
        "tool"
    ],
    "homepage": "https://github.com/Daniel-KM/Noid4Php",
    "autoload": {
        "psr-4": {
            "Noid\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
